{
  "name": "pino-gelf",
  "version": "1.0.0",
  "description": "Convert Pino logs to GELF format and gzip ready for upload to Graylog",
  "main": "index.js",
  "scripts": {
    "pretest": "eslint . --ext .js",
    "test": "nyc mocha -R spec --recursive",
    "posttest": "nyc report"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/findmypast/pino-gelf.git"
  },
  "author": "Donald Robertson <donaldarobertson89@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/findmypast/pino-gelf/issues"
  },
  "homepage": "https://github.com/findmypast/pino-gelf#readme",
  "nyc": {
    "include": [
      "index.js",
      "lib/*.js"
    ],
    "exclude": [
      "lib/pino-gelf.js"
    ],
    "extension": [
      ".js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "check-coverage": true,
    "lines": 85,
    "branches": 85,
    "functions": 85,
    "statements": 85
  },
  "dependencies": {
    "fast-json-parse": "^1.0.2",
    "fast-json-stringify": "^0.10.0",
    "pump": "^1.0.1",
    "readable-stream": "^2.1.5",
    "split2": "^2.1.0",
    "through2": "^2.0.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.8.1",
    "mocha": "^3.1.2",
    "nyc": "^8.3.2"
  }
}
